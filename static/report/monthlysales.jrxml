	<parameter name="date" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
    COALESCE(SUM(a.total) + b.amount, SUM(a.total)) AS total,
    EXTRACT(YEAR FROM a.date_transaction) AS transaction_year,
    MONTHNAME(a.date_transaction) AS MONTHS
FROM
    sales_payment a
LEFT JOIN cashonhand b ON
    DATE(a.date_transaction) = DATE(b.date_added)
WHERE
    EXTRACT(YEAR FROM a.date_transaction) = ' $P{date} '
GROUP BY
    transaction_year, MONTHNAME(a.date_transaction)]]>
	</queryString>
