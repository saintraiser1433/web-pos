	<parameter name="dates" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT   a.invoice_no,
                          b.discount,
                         DATE_FORMAT(b.date_transaction, '%b-%d-%Y') AS dates,
                          a.item_name,
                          a.quantity,
                          a.total_price,
                          CONCAT(c.last_name, ' ', c.first_name) AS fname
FROM sales a
	INNER JOIN sales_payment b ON 
	 a.invoice_no = b.invoice_no
	LEFT JOIN cashier c ON 
	  b.cashier_assign = c.cashier_id
WHERE 
	 date(b.date_transaction) = '$P{dates}']]>
	</queryString>
