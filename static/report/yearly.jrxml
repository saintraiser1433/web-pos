	<queryString>
		<![CDATA[SELECT
    COALESCE(SUM(a.total) + b.amount, SUM(a.total)) AS totals,
    DATE_FORMAT(a.date_transaction, '%Y') AS `YEAR`
FROM
    sales_payment a
LEFT JOIN cashonhand b ON
    DATE_FORMAT(a.date_transaction, '%Y') = DATE_FORMAT(b.date_added, '%Y')
GROUP BY
    DATE_FORMAT(a.date_transaction, '%Y')]]>
	</queryString>
